{
  "name": "niu-tools",
  "private": true,
  "type": "module",
  "scripts": {
    "rm": "node ./scripts/rm.js",
    "gen": "tsx ./scripts/gen-version.ts",
    "dev": "cd examples/my-app && pnpm dev",
    "dev:core": "pnpm run gen && pnpm --filter @niu-tools/core dev",
    "build": "pnpm run rm && pnpm run gen && pnpm -r run build",
    "docs:dev": "vitepress dev packages",
    "docs:build": "vitepress build packages",
    "docs:serve": "vitepress serve packages",
    "publish": "tsx ./scripts/publish.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@internal/tsconfig": "workspace:*",
    "@niu-tools/browser": "workspace:*",
    "@niu-tools/core": "workspace:*",
    "@niu-tools/loadconfig": "workspace:*",
    "@niu-tools/node": "workspace:*",
    "@niu-tools/request": "workspace:*",
    "@niu-tools/shared": "workspace:*",
    "@niu-tools/uniapp": "workspace:*",
    "@niu-tools/vue3": "workspace:*",
    "@types/node": "^18.16.19",
    "@vitepress-demo-preview/component": "^2.3.1",
    "@vitepress-demo-preview/plugin": "^1.2.0",
    "concurrently": "^7.6.0",
    "del": "^7.0.0",
    "fast-glob": "^3.3.0",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "lodash-es": "^4.17.21",
    "tsc-alias": "^1.8.7",
    "tsx": "^3.12.7",
    "turbo": "^1.10.7",
    "typescript": "^4.9.5",
    "vitepress": "1.0.0-alpha.60",
    "vitest": "^0.29.8",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "@manypkg/get-packages@1.1.3": "patches/@manypkg__get-packages@1.1.3.patch"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
